        <script>
            function set_cookie( cookie_name, cookie_value, lifespan_in_days, valid_domain ) {
                  var domain_string = valid_domain ? ("; domain=" + valid_domain) : '' ;
                  document.cookie = cookie_name + "=" + encodeURIComponent( cookie_value ) +
                      "; max-age=" + 60 * 60 * 24 * lifespan_in_days +
                      "; path=/" + domain_string ;
            }
            function get_cookie ( cookie_name )
            {
              // http://www.thesitewizard.com/javascripts/cookies.shtml
              var cookie_string = document.cookie ;
              if (cookie_string.length != 0) {
                var cookie_value = cookie_string.match ( '(^|;)[\s]*' + cookie_name + '=([^;]*)' );
                return decodeURIComponent ( cookie_value[2] ) ;
              }
              return '' ;
            }

            $(document).ready( function(){
                console.log(($(".homecontent").width() - $("#socialmedia").width()))
                $("#socialmedia").css("margin-left",($(".homecontent").width() - $("#socialmedia").width())/2)
                $("#cssmenu").css("margin-left", ($("#navcol").width() - $("#cssmenu").width())/2)
                if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) == true ) {
                    window.location = "http://development.thewarroom.divshot.io/indexmobile.html";
                }


                        $("#openVideo").remove()
                        $("#logo").css("opacity","1.0")
                        $("#logo").css("margin-left","40%")
                        $("#logo").css("margin-top","2%")
                        $("#logo").css("width","20%")
                        $(".secondarycol").css("opacity","1.0")
                        $(".secondarycol").css("display","inline")
                        setTimeout(function() {
                            console.log($("#socialmedia").width() )
                                           $("#socialmedia").css("margin-left",($(".homecontent").width() - $("#socialmedia").width())/2) 
                        }, 40); // After 5 secs    
                if (false) {
                    set_cookie("visitor","true", 365, "divshot.io")
                        if ($(window).width() > 1920) {
                            $("#openVideo").attr("width","1920px")   
                            $("#openVideo").attr("height","1080px")   
                            $("#openVideo").css("margin-left",($(window).width() - 1920)/2)   
                            $("#openVideo").css("margin-top",($(window).height() - 1080)/2)   
                        }
                        $("#openVideo").on("timeupdate", function(e) {
                            if (this.currentTime > 3) {
                                $("#logo").addClass("opacityanimationlogo")
                                $("#logo").css("opacity","1.0")
                            }
                        });
                        $("#openVideo").bind("ended", function() {
                                $("#socialmedia").css("margin-left",($(".homecontent").width() - $("#socialmedia").width())/2)
                                $("#openVideo").addClass("opacityanimationvideo")
                                $("#openVideo").css("opacity","0.0")
                                $("#logo").addClass("flyupanimationlogo")
                                $("#logo").css("margin-left","40%")
                                $("#logo").css("margin-top","2%")
                                $("#logo").css("width","20%")
                                $("#openVideo").remove()
                                $(".secondarycol").addClass("displayanimationsecondarycol")
                                $(".secondarycol").css("opacity","1.0")
                                $(".secondarycol").css("display","inline")

                        })
                }
            })
        </script>
