<h1> Signin </h1>

<%= form_for(:sessions, url: sessions_path) do |f|%>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in" %>

<%end%>
    <p>New user? <%= link_to "Sign up", signup_path %></p>
